<% if (theme.toc.enable) { %>
<section class="widget-toc widget-item layout-margin content-padding--primary soft-size--large soft-style--box sticky_element">
  <div class="widget-title">
    <%- partial("../_svg/icon-toc.svg") %>
    <span>Content</span>
  </div>
  <div class="widget-body">
    <%- toc(page.content, {
      max_depth: theme.toc.max_depth,
      min_depth: theme.toc.min_depth,
      list_number: false
    }) %>
  </div>
</section>
<% } %>